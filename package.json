{
  "config": {
    "blanket": {
      "data-cover-never": [
        "node_modules",
        "/test"
      ],
      "pattern": [
        "/controllers",
        "/models",
        "/workers"
      ]
    }
  },
  "name": "footbl",
  "version": "2.2.0",
  "scripts": {
    "test": "NODE_ENV=test mocha test/* --require blanket -R mocha-spec-cov-alt",
    "prestart": "apidoc -i controllers/ -o docs/",
    "start": "cluster index.js",
    "coverage": "mocha test/* -R html-cov --require blanket > coverage.html"
  },
  "engines": {
    "node": "0.10.21"
  },
  "dependencies": {
    "apidoc": "^0.13.1",
    "async": "^1.4.2",
    "auth": "footbl/auth.git",
    "body-parser": "^1.14.1",
    "cluster-start": "^0.1.4",
    "express": "^4.13.3",
    "fbgraph": "^1.1.0",
    "mandrill-api": "^1.0.45",
    "mongoose": "^4.1.9",
    "mongoose-json-select": "^0.2.1",
    "nconf": "^0.8.1",
    "push": "footbl/push.git",
    "request": "^2.64.0"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "mocha": "^2.1.0",
    "mocha-spec-cov-alt": "^0.2.4",
    "nock": "^2.13.0",
    "should": "^7.1.0",
    "supertest": "^1.1.0"
  }
}
