{
  "config": {
    "blanket": {
      "data-cover-never": [
        "node_modules",
        "/test"
      ],
      "pattern": [
        "/controllers",
        "/models",
        "/workers"
      ]
    }
  },
  "name": "footbl",
  "version": "2.0.1",
  "scripts": {
    "test": "NODE_ENV=test mocha test/ --require blanket -R mocha-spec-cov-alt",
    "prestart": "apidoc -i controllers/ -o docs/",
    "start": "cluster index.js"
  },
  "engines": {
    "node": "0.10.21"
  },
  "dependencies": {
    "async": "^0.7.0",
    "body-parser": "1.0.x",
    "cluster-start": "^0.1.4",
    "express": "^4.1.1",
    "fbgraph": "^0.2.10",
    "mandrill-api": "^1.0.39",
    "method-override": "^1.0.0",
    "mongoose": "3.x.x",
    "mongoose-json-select": "0.2.x",
    "nconf": "0.6.x",
    "node-freegeoip": "0.0.1",
    "pretty-error": "^1.1.1",
    "redis": "0.10.x",
    "request": "^2.39.0",
    "slug": "^0.5.0",
    "verror": "^1.4.0"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "mocha": "^1.20.1",
    "mocha-spec-cov-alt": "^0.1.1",
    "nock": "^0.42.1",
    "should": "^4.0.4",
    "supertest": "^0.13.0"
  }
}
