{
  "config": {
    "blanket": {
      "data-cover-never": [
        "node_modules",
        "/test"
      ],
      "pattern": [
        "/controllers",
        "/models",
        "/workers"
      ]
    }
  },
  "name": "footbl",
  "version": "2.2.0",
  "scripts": {
    "test": "NODE_ENV=test mocha test/* --require blanket -R mocha-spec-cov-alt",
    "prestart": "apidoc -i controllers/ -o docs/",
    "start": "cluster index.js",
    "coverage": "mocha test/* -R html-cov --require blanket > coverage.html"
  },
  "engines": {
    "node": "0.10.21"
  },
  "dependencies": {
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "mocha": "^2.0.1",
    "mocha-spec-cov-alt": "^0.2.3",
    "nock": "^0.42.1",
    "should": "^4.0.4",
    "supertest": "^0.13.0"
  }
}
